{
  "name": "Envoy MQTT Failover",
  "url": "https://github.com/7290903/addon-repository",
  "version": "1.0.1",
  "slug": "envoy_mqtt",
  "description": "TCP-фейловер между несколькими MQTT-брокерами через Envoy Proxy",
  "arch": ["amd64", "armv7", "armhf", "aarch64"],
  "startup": "services",
  "boot": "auto",
  "host_network": true,
  "map": ["config:rw"],
  "tmpfs": true,
  "options": {
    "port": 1885,
    "brokers": []
  },
  "schema": {
    "port": "int(1, 65535)",
    "brokers": [
      "str"
    ]
  }
}
